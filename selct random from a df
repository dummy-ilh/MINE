import pandas as pd

# Assuming you have a DataFrame 'df' with columns 'ID', 'predictions', and 'actual_labels'

# Filter false positives
false_positives_df = df[(df['predictions'] == 1) & (df['actual_labels'] == 0)]

# Sample 1000 false positive samples
sample_size = 1000
selected_samples = false_positives_df.sample(n=sample_size, random_state=42)  # Set an appropriate random state

print(selected_samples)
