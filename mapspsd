import pandas as pd

# Example dataframe
df = pd.DataFrame({
    "tag": ["x", "y", "z", "x", "y", "z"]
})

# Mapping dictionaries
map_a = {"x": "x1", "y": "y1", "z": "z1"}
map_b = {"x": "x2", "y": "y2", "z": "z2"}

# Apply mapping
df["a"] = df["tag"].map(map_a)
df["b"] = df["tag"].map(map_b)

print(df)
